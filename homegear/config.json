{
	"name": "homegear",
	"version": "1.0.5",
	"slug": "homegear",
	"description": "Homegear as a Hassio add-on",
	"webui": "http://[HOST]:[PORT:2001]/admin",
	"panel_icon": "mdi:home-lightbulb",
	"arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
	"startup": "before",
	"boot": "auto",
	"auto_uart": "true",
	"gpio": true,
	"privileged": ["SYS_ADMIN", "SYS_RAWIO"],
	"map": ["config:rw", "share:rw", "ssl:rw"],
	"ports": {
		"2001/tcp": 2001,
		"2002/tcp": 2002,
		"2003/tcp": 2003,
		"2005/tcp": 2004
	},
	"environment": {},
	"options": {},
	"schema": {}
}
